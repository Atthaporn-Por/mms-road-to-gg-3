{
  "presets": [
    "babel-preset-expo",
    "react-native-dotenv"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "underscore": "lodash"
      }
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "underscore": "lodash"
          }
        }],
        "transform-react-jsx-source"
      ]
    },
    "production": {
      "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "underscore": "lodash"
          }
        }]
      ]
    }
  }
}
